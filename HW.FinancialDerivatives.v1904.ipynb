    "class Curve:\n",
    "    def __init__(self, data, settlement, Today):\n",
    "        self.data = data\n",
    "        self.settlement = settlement\n",
    "        self.Today = Today\n",
    "        self.curve = data.copy()\n",
    "        self.set_dates()\n",
    "\n",
    "    def set_dates(self):\n",
    "        dt = self.settlement\n",
    "        self.curve.loc[:,'maturity'] = self.curve.loc[:,'end']\n",
    "\n",
    "    def df(self, date):\n",
    "        x = abs((date - self.Today).days)\n",
    "        return np.interp(x, self.curve.days_inter, self.curve.df)\n",
    "    \n",
    "    def fwd(self, init, maturity):\n",
    "        if init == self.settlement:\n",
    "            return self.curve.set_index('maturity').loc[maturity, 'par_rate']\n",
    "        if init < self.settlement:\n",
    "            return 0\n",
    "            \n",
    "        \n",
    "        df0 = self.df(init)\n",
    "        df1 = self.df(maturity)\n",
    "        dcf = (maturity - init).days / 360\n",
    "        return (df0 / df1 - 1) / dcf\n"
   ]
