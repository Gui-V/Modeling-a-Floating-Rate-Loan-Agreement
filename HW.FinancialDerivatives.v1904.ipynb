    "def float_leg_pv (notional,today, start, maturity, Mkt_data, resposta={}):\n",
    "    \n",
    "    #função 1\n",
    "    market_data= mkt_df(today, start, maturity,Mkt_data)\n",
    "    \n",
    "    #chamar classe\n",
    "    curve = Curve(market_data, start, today)\n",
    "    \n",
    "    #Calcular float_leg\n",
    "    pd.options.display.float_format = '{:,.4f}'.format\n",
    "    \n",
    "    global schedule2\n",
    "    global dates2\n",
    "    def schedule2(start, end, freq=6):            \n",
    "        def advance2(dt, months):\n",
    "            try:\n",
    "                return date(dt.year, dt.month + months, dt.day)\n",
    "            except:\n",
    "                return date(dt.year + 1, dt.month - months, dt.day)\n",
    "        dates2 = [start]\n",
    "        while dates2[-1] <= end:\n",
    "            dates2.append(advance2(dates2[-1], 6))\n",
    "        dates2=[today]+[today+timedelta(days=1)]+dates2\n",
    "        return dates2[:-1]\n",
    "    \n",
    "    dates2 = schedule2(start, maturity)\n",
    "    \n",
    "    float_leg=0\n",
    "    float_leg = pd.DataFrame({\n",
    "        'notional': notional,\n",
